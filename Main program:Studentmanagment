package program;

class StudentManagementSystem{

private String Student;

private int Marks;

StudentManagementSystem(String Student,int Marks){

this.Student= Student;

this.Marks=Marks;

}

public String getStudent (){

return Student;

}

public int getMarks () {

return Marks;

}

public void setResult(int Result) {

Marks += Result;

System.out.println("Display Result:"+ Result);

}

public class Student {



public static void main(String[] args) {

	StudentManagementSystem in= new StudentManagementSystem("Sandhya",98+ 94 + 100);

System.out.println("Student Name:"+in.getStudent());

System.out.println("Student Mark:"+ in.getMarks());

in.setResult(96 );



}

}



}
