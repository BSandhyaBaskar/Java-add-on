import java.util.Scanner;
class Main
{
	public static void main(String[] args)
	{
		
		System.out.print("Enter the starting value: ");
		Scanner in=new Scanner(System.in);
		int a =in.nextInt();
		System.out.print("Enter the ending value: ");
		int b =in.nextInt();
		
		for(int i=a;i<b;i++){
			if(i%2==0)
				System.out.println(i);
		}
	}
}
