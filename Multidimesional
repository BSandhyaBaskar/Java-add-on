package program;

public class Multidimensional {
 public static void main(String[] args) {
        int[][] array = {{1, 2},{10, 20}};
        System.out.println("Multidimensional Array:");
        for (int i = 0; i < 2; i++) {
            for (int j = 0; j < 2; j++) {
                System.out.print(array[i][j] + " ");
            }
            System.out.println(); 
        }
    }
}
